{
    "name": "vscode-changelogmanager",
    "displayName": "Changelog Manager",
    "description": "Manage the changelog for your application",
    "publisher": "carlocardella",
    "author": {
        "name": "Carlo Cardella",
        "url": "https://cloudnotes.io"
    },
    "version": "0.1.2",
    "preview": true,
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "changelog",
        "snippets",
        "keepachangelog",
        "snippet",
        "keep a changelog",
        "changes",
        "log",
        "markdown",
        "readme",
        "readme.md",
        "changelog.md"
    ],
    "icon": "clipboard.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/carlocardella/vscode-ChangelogManager.git"
    },
    "bugs": {
        "url": "https://github.com/carlocardella/vscode-ChangelogManager/issues"
    },
    "license": "MIT",
    "contributes": {
        "snippets": [
            {
                "language": "markdown",
                "path": "./Snippets/snippetsMarkdown.json"
            },
            {
                "language": "plaintext",
                "path": "./Snippets/snippetsPlainText.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "unit-tests": "tsc -w -p ./"
    },
    "dependencies": {
        "assert": "^2.0.0",
        "eslint": "^7.32.0",
        "mocha": "^9.1.1",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.2",
        "vscode": "^1.1.37",
        "vscode-test": "^1.6.1",
        "webpack": "^5.52.0",
        "webpack-cli": "^4.8.0"
    }
}
