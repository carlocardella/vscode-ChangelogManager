{
    "name": "vscode-changelogmanager",
    "displayName": "Changelog Manager",
    "description": "Manage the changelog for your application",
    "publisher": "carlocardella",
    "author": {
        "name": "Carlo Cardella",
        "url": "https://cloudnotes.io"
    },
    "version": "0.1.0",
    "preview": true,
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "changelog",
        "snippets",
        "keepachangelog",
        "snippet",
        "keep a changelog",
        "changes",
        "log",
        "markdown",
        "readme",
        "readme.md",
        "changelog.md"
    ],
    "icon": "clipboard.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/carlocardella/vscode-ChangelogManager.git"
    },
    "bugs": {
        "url": "https://github.com/carlocardella/vscode-ChangelogManager/issues"
    },
    "license": "MIT",
    "contributes": {
        "snippets": [
            {
                "language": "markdown",
                "path": "./Snippets/snippetsMarkdown.json"
            },
            {
                "language": "plaintext",
                "path": "./Snippets/snippetsPlainText.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "unit-tests": "tsc -w -p ./"
    },
    "devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	}
}
